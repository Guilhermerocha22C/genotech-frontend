{
    "version": 2,
    "builds": [
      {
        "src": "Backend/src/app.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": [
            "Backend/src/**",
            "Backend/package.json"
          ]
        }
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "Backend/src/app.js" },
      { "src": "/(.*)", "dest": "Frontend/$1" }
    ]
  }